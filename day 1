UNARY OPERATOR:
public class UnaryExample {
    public static void main(String[] args) {
        int a = 5;
        boolean flag = false;

        System.out.println(+a);   
        System.out.println(-a); 
        System.out.println(++a); 
        System.out.println(a++); 
        System.out.println(--a);  
        System.out.println(a--);  
        System.out.println(!flag); 
        System.out.println(~a);   
    }
}
